<h1>Invitations</h1>

<table class="table table-bordered">

    <thead>

        <tr>

            <th>ID</th>
            <th>Email</th>

        </tr>

        {{#each model as |invitation|}}

            <tr>

                <th>{{invitation.id}}</th>
                <td>{{invitation.email}}</td>

            </tr>

        {{/each}}

    </thead>

</table>