<h1>Contacts</h1>

<table class="table table-bordered">

    <thead>

        <tr>

            <th>Email</th>
            <th>Message</th>

        </tr>

        {{#each model as |contact|}}

            <tr>

                <td>{{contact.email}}</td>
                <td>{{contact.message}}</td>

            </tr>

        {{/each}}

    </thead>

</table>